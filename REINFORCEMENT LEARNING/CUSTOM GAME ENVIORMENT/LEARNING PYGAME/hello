import pygame as pg
import sys
import pymunk as pm
from pymunk import pygame_util

screen=pg.display.set_mode((800,600))
screen.fill((0,0,0))


while True:
    for event in pg.event.get():
        if event.type==pg.QUIT:
            pg.quit()
            sys.exit()
    
    
    
    screen.fill("white")
   
    
    #for i in range(10):
    #    pg.draw.circle(screen,(255,0,0),(i*10,50),5)
    pg.display.update()